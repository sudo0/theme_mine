        },
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "foreground": "#34ced3"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#42d174"
            }
        },
        {
            "name": "Numbers",
